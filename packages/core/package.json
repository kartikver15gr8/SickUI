{
  "name": "@sickui/core",
  "version": "0.0.4",
  "description": "A modern, high-performance React component library built with TypeScript, Tailwind CSS, and Radix UI primitives",
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "typescript",
    "tailwind",
    "tailwindcss",
    "radix-ui",
    "accessible",
    "headless",
    "component-library",
    "sickui"
  ],
  "author": "SickUI Team",
  "homepage": "https://github.com/kartikver15gr8/SickUI#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kartikver15gr8/SickUI.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/kartikver15gr8/SickUI/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles/globals.css"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.323.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}